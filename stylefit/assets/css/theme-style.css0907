/*** demo popup form css ***/

.st-form .ap-dark-btn {
    font-family: "Helvetica Neue Medium", Sans-serif;
    font-size: 16px;
    font-weight: 700;
    line-height: 20px;
    letter-spacing: 1px;
    color: #FFF;
    background-color: #F6924E;
    padding: 12px 5px 12px 5px;
